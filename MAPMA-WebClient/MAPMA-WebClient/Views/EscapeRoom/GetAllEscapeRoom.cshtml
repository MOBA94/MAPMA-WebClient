@model IEnumerable<MAPMA_WebClient.EscRef.EscapeRoom>

@{
    ViewBag.Title = "GetAllEscapeRoom";
}

<h2>GetAllEscapeRoom</h2>

<p>
   @* @Html.ActionLink("Create New", "Create")*@
</p>
<table class="table">
    <tr>
        <th>
            id
        </th>
        <th>
            description
        </th>
        <th>
            maxClearTime
        </th>
        <th>
            name
        </th>
        <th>
            price
        </th>
        <th>
            rating
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.List) {
    <tr>
        <td>
            @item.escapeRoomID
        </td>
        <td>
            @item.description
        </td>
        <td>
            @item.maxClearTime
        </td>
        <td>
            @item.name
        </td>
        <td>
             @item.price
        </td>
        <td>
            @item.rating
        </td>
        <td>
            @Html.ActionLink("Book", "CreateBooking", "Booking", new {EscapeRoom =  item }, new { @class = "BookingController"})
            @Html.ActionLink("Details", "Details", new { id=item.escapeRoomID })
        </td>
    </tr>
}

</table>
